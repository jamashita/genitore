{
  "name": "@jamashita/genitore",
  "version": "1.4.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:jamashita/genitore.git"
  },
  "license": "MIT",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && concurrently \"npm:build:*\"",
    "build:cjs": "tsc -b tsconfig.cjs.json",
    "build:esm": "tsc -b tsconfig.esm.json",
    "clean": "rimraf dist && rimraf coverage",
    "commit": "git cz",
    "format": "eslint --fix 'src/**/*.{ts,tsx}'",
    "release": "semantic-release",
    "test": "jest test",
    "setup": "echo 'da'",
    "update": "yarn update:upgrade && yarn update:install",
    "update:install": "yarn install && yarn upgrade && yarn audit fix --force",
    "update:upgrade": "ncu --packageManager yarn --upgrade"
  },
  "dependencies": {
    "@jamashita/anden-error": "1.8.0",
    "@jamashita/anden-object": "1.8.0",
    "@jamashita/anden-type": "1.10.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "12.1.1",
    "@jamashita/eslint-plugin": "1.42.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.1",
    "@semantic-release/npm": "7.1.1",
    "@semantic-release/release-notes-generator": "9.0.2",
    "@types/jest": "26.0.23",
    "@types/sinon": "10.0.0",
    "concurrently": "6.0.2",
    "eslint": "7.25.0",
    "git-cz": "4.7.6",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "npm-check-updates": "11.5.9",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.2",
    "sinon": "10.0.1",
    "sort-package-json": "1.49.0",
    "ts-jest": "26.5.5",
    "typescript": "4.2.4"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": "npm is not supported"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  }
}
